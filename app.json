{
  "name": "Telegram_Bot",
  "description": "Eğitim amaçlı basit bir telegram botu.",
  "repository": "https://github.com/Turkce-Botlar-Sohbet/Telegram-Bot",
  "logo": "https://avatars.githubusercontent.com/u/71149025",
  "keywords": ["telegram", "bot"],
  "env": {
    "BOT_API": {
      "description": "@BotFather'dan oluşturduğunuz botun tokeni.",
      "value": ""
    },
    "URL": {
      "description": "Yukarıda app-name yerine yazdığınız adı yourappname ile değiştirin.",
      "value": "https://yourappname.herokuapp.com/"
    },
    "PORT": {
      "description": "Bu kalabilir. (İsteğe bağlı)",
      "value": "80",
      "required": false
    }
  },
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "formation": {
    "web": {
      "quantity": 1
    }
  }
}
